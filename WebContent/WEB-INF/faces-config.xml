<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>person</managed-bean-name>
  <managed-bean-class>demo.Person</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>name</property-name>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cadastroPessoaController</managed-bean-name>
  <managed-bean-class>projeto.controller.CadastroPessoaController</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>testes2</managed-bean-name>
  <managed-bean-class>projeto.controller.Testes2</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pprBean</managed-bean-name>
  <managed-bean-class>projeto.controller.pprBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>chartBean</managed-bean-name>
  <managed-bean-class>projeto.controller.ChartBean</managed-bean-class>
  <managed-bean-scope>none</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>calendarBean</managed-bean-name>
  <managed-bean-class>projeto.controller.CalendarBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>carouselBean</managed-bean-name>
  <managed-bean-class>projeto.controller.CarouselBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tableBean</managed-bean-name>
  <managed-bean-class>projeto.controller.TableBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>fileUploadController</managed-bean-name>
  <managed-bean-class>projeto.controller.FileUploadController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>maskController</managed-bean-name>
  <managed-bean-class>projeto.controller.MaskController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/pages/inputname.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>greeting</from-outcome>
   <to-view-id>/pages/greeting.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/home.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>testes2</from-outcome>
   <to-view-id>/pages/testes2.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>registerPerson</from-outcome>
   <to-view-id>/pages/registerPerson.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>personsView</from-outcome>
   <to-view-id>/pages/personsView.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>testes</from-outcome>
   <to-view-id>/pages/testes.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>prime</from-outcome>
   <to-view-id>/pages/prime.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/registerPerson.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/personsView.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>registerPerson</from-outcome>
   <to-view-id>/pages/registerPerson.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/prime.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Ajaxify_Select_Components</from-outcome>
   <to-view-id>/pages/prime/Ajaxify_Select_Components.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>calendar</from-outcome>
   <to-view-id>/pages/prime/calendar.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>carousel</from-outcome>
   <to-view-id>/pages/prime/carousel.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>chart</from-outcome>
   <to-view-id>/pages/prime/chart.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>exported</from-outcome>
   <to-view-id>/pages/prime/exported.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>dialog</from-outcome>
   <to-view-id>/pages/prime/dialog.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>dockItem</from-outcome>
   <to-view-id>/pages/prime/dockItem.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>editor</from-outcome>
   <to-view-id>/pages/prime/editor.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>upload</from-outcome>
   <to-view-id>/pages/prime/upload.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>imagensswisth</from-outcome>
   <to-view-id>/pages/prime/imagensswisth.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>ligthBox</from-outcome>
   <to-view-id>/pages/prime/ligthBox.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>mask</from-outcome>
   <to-view-id>/pages/prime/mask.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <!-- Configura o Spring 2.5 para trabalhar com JSF 1.2 -->
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
